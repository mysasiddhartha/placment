public class java1{
public  static void main(String[] args) {
    int y = 7;
    int x = 5;
    System.out.println("X is:"+x);
    System.out.println("Y is:" +y);
    
      
    
     
    byte b= 10;
    System.out.println("8 is:" +b);
    
     
    
    
     
    
    int i=b;
    
    System.out.println("I is :" +i);
    
    }
    }
    